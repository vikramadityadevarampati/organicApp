{
  "expo": {
    "name": "Organic Farm Management",
    "slug": "organic-farm-management-uae",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "farmmanagement",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Sustainable farming management system for UAE organic agriculture",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.organicfarm.management.uae"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#22C55E"
      },
      "package": "com.organicfarm.management.uae"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-camera"],
    "experiments": {
      "typedRoutes": true
    }
  }
}